#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>
#include <ctype.h>
#include "main.h"

/**
 * main - check the code
 *
 * Return: Always 0.
 */

int main(void)
{
printf("%d\n", mul(98, 1024));
printf("%d\n", mul(-402, 4096));
return (0);
}

/**
 * mul - multiply two integers
 * @a: input a
 * @b: input b
 *
 * Return: a x b
 */

int mul(int a, int b)
{
int y;
y = a * b;
return (y);
}
