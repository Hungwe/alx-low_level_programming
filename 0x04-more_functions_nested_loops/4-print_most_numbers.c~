#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <string.h>
#include <ctype.h>
#include "main.h"

/**
 * print_numbers - Print 0 to 9 skipping 2 & 4
 *
 */

void print_most_numbers(void)
{
int i;

for (i = 0; i < 10; i++)
{
if (i == 2 || i == 4)
{
continue;
}
else
{
printf("%d", i);
}
}
printf("\n");
}
